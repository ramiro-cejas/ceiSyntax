///[SinErrores]
class EstructurasControl {
    public void condicionalesCiclos() {
        int x = 5;
        if (x > 0) {
            System.out.println("x es positivo");
        } else {
            System.out.println("x es no positivo");
        }

        while (x > 0) {
            System.out.println("x es " + x);
            x--;
        }

        for (int i = 0; i < 3; i++) {
            System.out.println("IteraciÃ³n " + i);
        }
    }
}